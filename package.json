  "homepage": "https://peoplesoutreach.github.io/peoplesoutreach.org/",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.8",
    "bsky-embed": "^0.2.7"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.16.1",
    "gh-pages": "^6.3.0",
    "parcel": "^2.16.1",
    "svgo": "^4.0.0"
  }
}