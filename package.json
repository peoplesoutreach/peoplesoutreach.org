{
  "name": "parcel",
  "description": "Include Bootstrap's source Sass and individual JavaScript plugins with Parcel.",
  "version": "0.0.0",
  "private": true,
  "repository": "twbs/examples",
  "license": "MIT",
  "stackblitz": {
    "startCommand": "npm start"
  },
  "scripts": {
    "start": "parcel serve src/index.html --public-url / --dist-dir dist",
